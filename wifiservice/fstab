proc            /proc           proc    defaults          0       0
/dev/mmcblk0p1  /boot           vfat    defaults          0       2
/dev/mmcblk0p2  /               ext4    defaults,noatime,ro  0       1
/dev/mmcblk0p3	/var/lib/logmein-hamachi/	ext4	defaults,noatime,rw	0	0	
/dev/sda1	/home/pi/data	ext4	defaults,noatime,rw	0	0
# a swapfile is not a swap partition, so no using swapon|off from here on, use  dphys-swapfile swap[on|off]  for that
tmpfs           /tmp            tmpfs   defaults,noatime,mode=1777 0       0
tmpfs           /var/log        tmpfs   defaults,noatime,mode=0755 0       0
tmpfs           /var/lock       tmpfs   defaults,noatime,mode=0755 0       0
tmpfs           /home/pi/tmp    tmpfs   defaults,noatime,mode=0777 0       0
